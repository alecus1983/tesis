isbn = {9781467396820},
keywords = {Active network management,Demand Side,Distributed Generation,Management,Optimal Power Flow,Power Factor Control,Smart Grid.},
title = {{Reactive Power Management at Transmission / Distribution Interface}},
volume = {2015-Novem},
