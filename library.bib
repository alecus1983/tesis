keywords = {Heuristic algorithm,Optimal PMU placement (OPP),Phasor measurement unit (PMU),Power system observability},
pages = {214--228},
publisher = {Elsevier},
title = {{Application of heuristic algorithms to optimal PMU placement in electric power systems: An updated review}},
url = {http://dx.doi.org/10.1016/j.rser.2015.04.152},
volume = {50},
year = {2015}
}
