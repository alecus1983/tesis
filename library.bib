eprint = {1504.05916},
file = {:home/alejandro/ide{\_}usb/maestria/investigacion/mendeley/2016/2016 - Arghandeh et al. - On the definition of cyber-physical resilience in power systems - Renewable and Sustainable Energy Reviews.pdf:pdf},
isbn = {1539-6924},
issn = {18790690},
journal = {Renew. Sustain. Energy Rev.},
keywords = {Cyber-physical system,Microgrids,Power system,Resilience,Smart grids,Vulnerabilities},
pages = {1060--1069},
publisher = {Elsevier},
title = {{On the definition of cyber-physical resilience in power systems}},
url = {http://dx.doi.org/10.1016/j.rser.2015.12.193},
volume = {58},
year = {2016}
}
