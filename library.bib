journal = {Renew. Sustain. Energy Rev.},
keywords = {DG,Distribution system,Optimal planning},
number = {7},
pages = {5146--5165},
publisher = {Elsevier},
title = {{Optimal planning of distributed generation systems in distribution system: A review}},
url = {http://dx.doi.org/10.1016/j.rser.2012.05.020},
