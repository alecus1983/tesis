@article{Toman2016a,
abstract = {{\textcopyright} 2016 IEEE.In this work primary control loop of the microgrid (MG) hierarchy is implemented by decomposing converter voltage and current into two oppositely rotating reference frames. The double Synchronous reference frame method is used with a filter-based extraction to yield values of each sequence component. A distributed finite control set-model predictive control (DFCS-MPC) is extended and applied to control both sequences with one cost function. In this way the use of any parameters tuning, as in the classical PI controller is eliminated. The method exhibits features of the powerful MPC approach. Since an island mode is assumed, voltage and current control strategies are used. Power quality at the point of coupling is considerably improved by setting the voltage negative sequence component to zero in the cost function and nullifying the negative sequence current share of the relevant voltage-creating converter. Cases corresponding to different sharing conditions are studied. The validity of the proposed approach is verified under all considered conditions.},
author = {Toman, Riyadh T. and Asumadu, Johnson},
doi = {10.1109/PEDG.2016.7527075},
file = {:home/alejandro/ide{\_}usb/maestria/investigacion/mendeley/2016/2016 - Toman, Asumadu - Control of an islanded microgrid based on a distributed finite control set-model predictive control A double Syn.pdf:pdf},
isbn = {9781467386166},
journal = {2016 IEEE 7th Int. Symp. Power Electron. Distrib. Gener. Syst. PEDG 2016},
keywords = {Double synchrouns reference frame,Microgrid,distributed model predictive control,island mode},
pages = {0--7},
title = {{Control of an islanded microgrid based on a distributed finite control set-model predictive control: A double Synchronous reference frame approach}},
year = {2016}
}
@article{VanDerSchaft2016,
abstract = {Increasing integration of renewable energy sources in the power grid leads to a complete re-thinking of its operation. This necessitates the consideration of new modeling and analysis approaches, which can serve as natural starting points for robust and scalable control and design strategies. In this 'vision' paper we will highlight two topics within the broad area of power networks: the modeling and analysis of the synchronous generator, and the modeling and analysis of power networks using the swing equation as an approximate model for the generator. In both cases we will discuss a port-Hamiltonian formulation, which reflects the underlying physics of power flow and energy storage. Although the port-Hamiltonian model of the synchronous generator reveals a clear structure it still poses fundamental challenges for its non-zero steady state stability analysis. It is shown how the swing equation can be directly deduced from the power balance of the port-Hamiltonian model of the synchronous generator. Under the phasor assumption, this leads to a port-Hamiltonian model of power networks of generators, which enables a straightforward stability analysis and provides a starting point for control.},
author = {{Van Der Schaft}, Arjan and Stegink, Tjerk and Schaft, Arjan Van Der and Stegink, Tjerk},
doi = {10.1016/j.arcontrol.2016.04.017},
file = {:home/alejandro/ide{\_}usb/maestria/investigacion/mendeley/2016/2016 - Van Der Schaft, Stegink - Perspectives in modeling for control of power networks - Annual Reviews in Control.pdf:pdf},
issn = {13675788},
journal = {Annu. Rev. Control},
keywords = {Networks,Port-Hamiltonian system,Shifted passivity,Stab,Stability,Swing equation,Synchronous generator,port-hamiltonian system,synchronous generator},
pages = {119--132},
publisher = {Elsevier Ltd},
title = {{Perspectives in modeling for control of power networks}},
volume = {41},
year = {2016}
}
