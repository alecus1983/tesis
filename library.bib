issn = {03787796},
journal = {Electr. Power Syst. Res.},
keywords = {Capacitor switching,Clustering,ISODATA,Power distribution},
pages = {96--102},
publisher = {Elsevier B.V.},
title = {{Automatic capacitor bank identification in power distribution systems}},
url = {http://dx.doi.org/10.1016/j.epsr.2014.02.003},
volume = {111},
year = {2014}
}

