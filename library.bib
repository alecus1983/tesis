	isbn = {1905593368},
	journal = {Proc. Univ. Power Eng. Conf.},
	keywords = {AVC relays,Distribution network,Parallel transformers,TAPP scheme},
	number = {2},
	pages = {349--356},
	title = {{Systematic modelling and analysis of tapp voltage control schemes}},
	year = {2007}
}

