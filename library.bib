journal = {Int. J. Electr. Power Energy Syst.},
keywords = {Capacitor bank,Distribution and industrial networks,Reactive power control,Reactor},
pages = {67--73},
publisher = {Elsevier Ltd},
title = {{One approach for reactive power control of capacitor banks in distribution and industrial networks}},
url = {http://dx.doi.org/10.1016/j.ijepes.2014.02.039},
volume = {60},
year = {2014}
}
@article{Vkdulqj,
