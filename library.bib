doi = {10.1016/j.renene.2013.09.007},
file = {:home/alejandro/ide{\_}usb/maestria/investigacion/mendeley/2014/2014 - Perera et al. - Characterisation of flicker emission and propagation in distribution networks with bi-directional power flows - R.pdf:pdf},
issn = {09601481},
journal = {Renew. Energy},
keywords = {Doubly-fed induction generator (DFIG),Flicker,Power factor control,Voltage control,Voltage fluctuations,Wind generation},
pages = {172--180},
publisher = {Elsevier Ltd},
title = {{Characterisation of flicker emission and propagation in distribution networks with bi-directional power flows}},
